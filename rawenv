#4PKGSUFFIX=.tgz
#4PKGZIPCMD=gzip
#4INDEXFILE=INDEX
#5PKGSUFFIX=.tbz
#5PKGZIPCMD=bzip2
#5INDEXFILE=INDEX-5
ARCH=i386
MACHINE_ARCH=i386
XFREE86_VERSION=4
PORTSDIR=`realpath ##PB##/##JAIL##/ports`
SRCBASE=`realpath ##PB##/##JAIL##/src`
SRCPREFIX=`realpath ##PB##/##JAIL##/src`
OSVERSION=`awk '/^#define __FreeBSD_version/ {print $3}' < ^^SRCBASE^^/sys/sys/param.h`
OSREL=`awk 'BEGIN {FS="\""}; /^REVISION/ {print $2}' < ^^SRCBASE^^/sys/conf/newvers.sh`
BRANCH=`awk 'BEGIN {FS="\""}; /^BRANCH/ {print $2}' < ^^SRCBASE^^/sys/conf/newvers.sh`
__MAKE_CONF=##MAKE##
DISTDIR=##PB##/distfiles/##JAIL##
PACKAGES=##PB##/packages/##JAIL##
UNAME_m=i386
UNAME_n=marcuscom.com
UNAME_p=i386
UNAME_r=^^OSREL^^-^^BRANCH^^
UNAME_s=FreeBSD
UNAME_v="FreeBSD ^^OSREL^^-^^BRANCH^^ #0: `date`    marcus@marcuscom.com:/usr/src/sys/magic/kernel/path"
BATCH=1
PACKAGE_BUILDING=1
USA_RESIDENT=1
PORTOBJFORMAT=elf
PARALLEL_PACKAGE_BUILD=1
HAVE_MOTIF=1
FTP_PASSIVE_MODE=yes
